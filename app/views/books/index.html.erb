<h1>蔵書の一覧</h1>

<table class="books">
  <col class="title" />
  <col class="authors" />
  <col class="publish_year" />
  <% @books.each do |book| %>
  <tr>
    <td><%= link_to book.title, book %></td>
    <td><%= book.authors %></td>
    <td><%= book.publish_year %></td>
  </tr>
  <% end %>
</table>
